  "turboConsoleLog.addSemicolonInTheEnd": true,
  "turboConsoleLog.delimiterInsideMessage": "",
  "turboConsoleLog.includeFileNameAndLineNum": false,
  "turboConsoleLog.insertEmptyLineAfterLogMessage": false,
  "turboConsoleLog.insertEmptyLineBeforeLogMessage": false,
  "turboConsoleLog.insertEnclosingClass": false,
  "turboConsoleLog.insertEnclosingFunction": false,
  "turboConsoleLog.logFunction": "log",
  "turboConsoleLog.logMessagePrefix": "",
  "turboConsoleLog.logMessageSuffix": ":",
  "turboConsoleLog.logType": "log",
  "turboConsoleLog.quote": "`",
  "turboConsoleLog.wrapLogMessage": false,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.singleQuote": true,
    "prettier.semi": true,
    "prettier.arrowParens": "always",
    "prettier.tabWidth": 2,
    "prettier.useTabs": true
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "prettier.singleQuote": true,
    "prettier.semi": true,
    "prettier.arrowParens": "always",
    "prettier.tabWidth": 2,
    "prettier.useTabs": true
  },
  "editor.formatOnSave": true,
  "editor.wordWrap": "on",
  "cSpell.words": ["dbank", "Emojipedia", "exceljs", "Motoko", "networkidle"],
  "AutoPreview.serverURL": "http://localhost:5173/",
  "AutoPreview.background": "transparent",
  "[motoko]": {
    "editor.defaultFormatter": "dfinity-foundation.vscode-motoko"
  },
  "debug.javascript.autoAttachFilter": "always",
  "cucumberautocomplete.steps": [
    "features/step_definitions/**/*.js",
    "Playwright-BDD/step_definitions/**/*.{js,ts}"
  ],
  "[feature]": {
    "editor.defaultFormatter": "alexkrechik.cucumberautocomplete"
  },
  "github.copilot.enable": {
    "*": false
  },
  "cucumberautocomplete.syncfeatures": "features/**/*feature",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
